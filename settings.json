{
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "workbench.editorAssociations": {
        "*.11": "default",
        "{git,gitlens}:/**/*.{md,csv}": "default",
        "{git,gitlens}:/**/*.{md,csv,svg}": "default"
    },
    "security.workspace.trust.untrustedFiles": "open",
    "cSpell.userWords": [
        "AIMES",
        "CEPM",
        "curlparser",
        "exeption",
        "exitonclick",
        "fastapi",
        "nltk",
        "pydantic",
        "pymoo",
        "pyqrcode",
        "pyttsx",
        "QUATER",
        "reasigns",
        "scipy",
        "sess",
        "sklearn",
        "sqlalchemy",
        "termcolor",
        "tqdm"
    ],
    "git.autofetch": true,
    "javascript.updateImportsOnFileMove.enabled": "always",
    "git.confirmSync": false,
    "python.defaultInterpreterPath": "/opt/homebrew/bin/python3.12",
    "git.enableSmartCommit": true,
    "tabnine.experimentalAutoImports": true,
    "aiDocsAndComments.openAI.codeLanguage": "Python",
    "indentRainbow.colorOnWhiteSpaceOnly": true,
    "indentRainbow.excludedLanguages": [

        "plaintext"
    ],
    "indentRainbow.colors": [

        "rgba(255,255,64,0.07)",
        "rgba(127,255,127,0.07)",
        "rgba(255,127,255,0.07)",
        "rgba(79,236,236,0.07)"
    ],
    "workbench.iconTheme": "material-icon-theme",
    "[python]": {
        "editor.defaultFormatter": "ms-python.black-formatter"
    },
    "vscode-pets.theme": "forest",
    "python.analysis.addImport.exactMatchOnly": true,
    "python.analysis.autoFormatStrings": true,
    "python.analysis.autoImportCompletions": true,
    "python.analysis.completeFunctionParens": true,
    "python.analysis.diagnosticMode": "workspace",
    "python.analysis.inlayHints.functionReturnTypes": true,
    "python.analysis.inlayHints.variableTypes": true,
    "python.analysis.typeCheckingMode": "standard",
    "gitlens.ai.experimental.provider": "openai",
    "gitlens.ai.experimental.openai.model": "gpt-4-turbo-preview",
    "window.zoomLevel": 1,
    "terminal.integrated.enableMultiLinePasteWarning": true,
    "vscode-office.openOutline": true,
    "python.createEnvironment.trigger": "off",
    "gitlens.ai.experimental.model": "openai:gpt-4o",
    "workbench.activityBar.location": "top",
    "workbench.statusBar.visible": false,

    "vscode_custom_css.imports": [ "file:///Users/daniilparfenov/Documents/software/custom-vs/main.css", "file:///Users/daniilparfenov/Documents/software/custom-vs/main.js" ],

    "editor.copyWithSyntaxHighlighting": false,
    "diffEditor.ignoreTrimWhitespace": false,
    "editor.emptySelectionClipboard": false,
    "workbench.editor.enablePreview": false,
    "window.newWindowDimensions": "inherit",
    "editor.multiCursorModifier": "alt",
    "files.trimTrailingWhitespace": true,
    "diffEditor.renderSideBySide": false,
    "editor.snippetSuggestions": "top",
    "editor.detectIndentation": false,
    "window.nativeFullScreen": true,
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "workbench.editor.showTabs": "none",
    "editor.minimap.enabled": false,
    "editor.lineNumbers": "on",
    "editor.guides.indentation": false,

    // Silence the Noise
    "breadcrumbs.enabled": false,
    "scm.diffDecorations": "none",
    "editor.hover.enabled": true,
    "editor.matchBrackets": "never",
    "workbench.tips.enabled": false,
    "editor.colorDecorators": false,
    "git.decorations.enabled": false,
    "workbench.startupEditor": "none",
    "editor.lightbulb.enabled": "off",
    "editor.selectionHighlight": false,
    "editor.overviewRulerBorder": false,
    "editor.renderLineHighlight": "none",
    "editor.occurrencesHighlight": "off",
    "problems.decorations.enabled": false,
    "editor.renderControlCharacters": false,
    "editor.hideCursorInOverviewRuler": true,
    "editor.gotoLocation.multipleReferences": "goto",
    "editor.gotoLocation.multipleDefinitions": "goto",
    "editor.gotoLocation.multipleDeclarations": "goto",
    "workbench.editor.enablePreviewFromQuickOpen": false,
    "editor.gotoLocation.multipleImplementations": "goto",
    "editor.gotoLocation.multipleTypeDefinitions": "goto",

    // Typography
    "editor.fontFamily": "Geist Mono, JetBrains Mono, Fira Code",
    "editor.suggestFontSize": 16,
    "editor.suggestLineHeight": 30,
    "terminal.integrated.lineHeight": 1,
    "terminal.integrated.fontSize": 14,
    "editor.hover.delay": 100,
    "files.autoSave": "onFocusChange",
    "workbench.colorCustomizations": {
        "terminal.background": "#00000000"
    }

}
